{
  "name": "vue-i18n-fetch",
  "version": "1.2.0",
  "description": "Async store and load vue-i18n messages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src/**",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klarkc/vue-i18n-fetch.git"
  },
  "keywords": [
    "Vue",
    "i18n",
    "vue-i18n",
    "localization",
    "translate",
    "internationalization",
    "server",
    "api"
  ],
  "author": {
    "name": "Walker Leite",
    "email": "walker@praiseweb.com.br"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/klarkc/vue-i18n-fetch/issues"
  },
  "homepage": "https://github.com/klarkc/vue-i18n-fetch#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "dependencies": {
    "vue-demi": "^0.7.5",
    "vue-i18n": "9"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "release-it": {
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "release-it": "^14.6.1",
    "typescript": "^4.2.4",
    "vue": "^3.0.0"
  }
}
